import java.util.Scanner;

class Main {


public static void main(String[] args) {

java.util.Scanner valor = new Scanner(System.in);

System.out.println("Ingrese un valor: ");

int nu= valor.nextInt();


double cuadrado = Math.pow(nu, 2);



System.out.println("El valor ingresado al cuadrado es: " + cuadrado);

 }
}
