class Main {

public static void main(String[] args) {

Scanner valor = new Scanner(System.in);

System.out.println("Ingrese el primer numero: "); int nu1 = valor.nextInt();


System.out.println("Ingrese el segundo numero: "); int nu2 = valor.nextInt();

int suma = nu1 + nu2;

System.out.println("La suma de los valores ingresados es:" + suma);

  }
}
